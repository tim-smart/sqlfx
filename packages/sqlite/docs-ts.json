{
  "exclude": ["src/internal/**/*.ts"],
  "theme": "mikearnaldi/just-the-docs",
  "parseCompilerOptions": {
    "noEmit": true,
    "strict": true,
    "target": "es2015",
    "lib": ["es2015"],
    "paths": {
      "@sqlfx/sql": ["../sql/src/index.ts"],
      "@sqlfx/sql/*": ["../sql/src/*"],
      "@sqlfx/sqlite": ["./src/index.ts"],
      "@sqlfx/sqlite/*": ["./src/*"]
    }
  },
  "examplesCompilerOptions": {
    "noEmit": true,
    "strict": true,
    "target": "es2015",
    "lib": ["es2015"],
    "paths": {
      "@sqlfx/sql": ["../sql/src/index.ts"],
      "@sqlfx/sql/*": ["../sql/src/*"],
      "@sqlfx/sqlite": ["./src/index.ts"],
      "@sqlfx/sqlite/*": ["./src/*"]
    }
  }
}
